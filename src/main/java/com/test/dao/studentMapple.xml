<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.mybatis.dao.StudentMapple">
   <insert id="insert" parameterType="com.test.mybatis.pojo.Student" >
   insert into t_student(name,age,sex) values(#{name},#{age},#{sex})
   </insert>
   <select id="select" resultType="com.test.mybatis.pojo.Student" parameterType="int">
    select s.name from t_student s  where s.id=#{id}
   </select>
</mapper>